{
  "name": "react-client",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "build": "webpack -d && cp src/index.html dist/index.html && webpack-dev-server --content-base src/ --inline --host 0.0.0.0 --port 5000 --history-api-fallback",
    "build-prod": "webpack -p && cp src/index.html dist/index.html"
  },
  "license": "MIT",
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "babel-core": "6",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "webpack": "^3.9.1",
    "webpack-dev-server": "^2.9.5"
  }
}
